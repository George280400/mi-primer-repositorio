package Implementacion;

import java.util.ArrayList;
import java.util.List;

import Principal.IMetodos;





public class implementacionGenerica implements IMetodos{

	//declarar lista
	List<Object> lista = new ArrayList<Object>();

	@Override
	public void guardar(Object Object) {
		
		lista.add(Object);
	}

	@Override
	public Object buscar(Object Object) {
		
		return null;
	}

	@Override
	public void editar(int indice, Object Object) {
		
		lista.set(indice, Object);
		
	}

	@Override
	public void eliminar(int indice, Object Object) {
		
		lista.remove(indice);
	}

	@Override
	public void mostrar() {
		
		
	}
}
